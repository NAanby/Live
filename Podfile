platform :ios, '9.0'

source 'https://github.com/CocoaPods/Specs.git'

def pods
# 网络
pod 'AFNetworking'
# 图片
pod 'SDWebImage'
# UI框架
pod 'YYKit'
# 圆角
pod 'ZYCornerRadius'
# 提示
pod 'MBProgressHUD'
pod 'LYEmptyView'
# 下拉刷新
pod 'MJRefresh'
# 布局
pod 'Masonry'
# 直播工具
#pod 'LFLiveKit'
pod 'PLPlayerKit'
# 解析
pod 'MJExtension'
pod 'BarrageRenderer'
# 轮播图
pod 'TYCyclePagerView'

end

target 'ProApp' do
    pods
end
